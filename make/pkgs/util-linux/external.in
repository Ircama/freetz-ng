config EXTERNAL_FREETZ_PACKAGE_UTIL_LINUX
	depends on EXTERNAL_ENABLED && FREETZ_PACKAGE_UTIL_LINUX
	bool "util-linux"
	default n
	help
		externals the following file(s):
		 /sbin/blkid-util-linux
		 /sbin/lsblk
		 /sbin/findmnt
		 /sbin/losetup-util-linux
		 /sbin/mkswap-util-linux
		 /sbin/swapon-util-linux
		 /sbin/swapoff-util-linux

